extends /layout


block content
	- var blogPosts = 3;
	//- Main Hero Header
	section#home-hero.cover.height-80.imagebg.parallax.overlay-blue(data-overlay="5")
		//- background image
		div.background-image-holder(style="background: url(/img/header/home.jpg); opacity: 1;") 
			//-background image for smaller devices
			img.attachment-full.size-full(width="1600" height="1067" src="/img/header/home.jpg" alt="" srcset="/img/header/home.jpg 1600w, /img/header/home.jpg 300w, /img/header/home.jpg 768w, /img/header/home.jpg 1024w" sizes="(max-width: 1600px) 100vw, 1600px")
		div.container.pos-vertical-center
			div.row
				div.col-sm-12
					h1.hr-green-after.text-left COMING HOME IS A DREAM WE MAKE COME TRUE
						//-div.wpb_single_image.wpb_content_element.vc_align_center
						//-figure.wpb_wrapper.vc_figure
							div.vc_single_image-wrapper.vc_box_border_grey
								img.vc_single_image-img.attachment-full(width="1634" height="448" src="/img/stack/headline-1.png" alt="" srcset="/img/stack/headline-1.png 1634w, /img/stack/headline-1.png 300w, /img/stack/headline-1.png 768w, /img/headline-1.png 1024w" sizes="(max-width: 1634px) 100vw, 1634px")
			//-div.wpb_text_column.wpb_content_element
				div.wpb_wrapper
					h5.text-left= contentful.Home[0].headerSubtext
					
				//-p(style="text-align: center;")
					a.btn.btn--primary.type--uppercase(href="#")
						span.btn__text VIEW THE DEMOS
	

		//- home login
		//-include partial/home_login

	//- Icon Navigation
	
	//-if (contentful.Alerts[0] != undefined)
		- var date = new Date
		div.container.mt--3
			div.row
				div.col-md-12
					for alert in contentful.Alerts
						- var dateFrom = new Date(alert.dateFrom)
						- var dateTo = new Date(alert.dateTo)
						if (date >= dateFrom && date <= dateTo)
							div.alert.bg--error
								div.alert__body
									h5= alert.title
									p= alert.content
									//-p #{alert.dateFrom} - #{alert.dateTo}
								div.alert__close ×
	section#home-landing
		.container
			.row.text-center
				//-.col-sm-4
					img(src="img/icons/piggybank.png" alt="piggy bank")
				.col-sm-12
					h3.color--blue A simple start to dreaming big
			.row
				.col-sm-12
					p.
						As a customer, you’re everything to us. So we’ve built a new kind of bank for you and the way you live now. Amazing new technology. Products and services that feel effortless. And smart people to help you meet your goals. TFED is more than a bank, it’s a state of mind.

					//-a.btn.btn-blue(href="/loans") 
						span.btn__text Loans

	section#full-width-ads.unpad--bottom
		.row.text-center
			a(href='#')
				.col-sm-6.imagebg.height-60.overlay-green(data-overlay="5")
					.background-image-holder
						img(src="/img/test3.jpg")
					.pos-vertical-center
						h3 Low Auto Rates
						a.btn.btn-white(href="#")
							span.btn__text Learn More
			a(href='#')
				.col-sm-6.imagebg.height-60.overlay-blue(data-overlay="5")
					.background-image-holder
						img(src="/img/test2.jpg")
					.pos-vertical-center
						h3 Holiday Skip-A-Pay
						a.btn.btn-white(href="#")
							span.btn__text Learn More

	section#our-services
		.container
			.row.text-center
				.col-sm-12.mb--3
					h3.color--blue Banking made easy
			.row
				a(href='/spend')
					.col-sm-3.col-xs-6.inline-block-xs
						img.block(src="img/icons/checking-icon.png")
						h6.text-center CHECKING
				a(href='/save')
					.col-sm-3.col-xs-6.inline-block-xs
						img.block(src="img/icons/savings-icon.png")
						h6.text-center SAVINGS
				a(href='/loans/credit-cards')
					.col-sm-3.col-xs-6.inline-block-xs
						img.block(src="img/icons/credit-card-icon.png")
						h6.text-center CREDIT CARD
				a(href='/loans/auto-loans')
					.col-sm-3.col-xs-6.inline-block-xs
						img.block(src="img/icons/auto-loan-icon.png")
						h6.text-center AUTO LOANS
		.container
			.row
				a(href='/loans/home-loans')
					.col-sm-3.col-xs-6.inline-block-xs
						img.block(src="img/icons/home-loan-icon.png")
						h6.text-center CALCULATOR
				a(href='/digital-banking')
					.col-sm-3.col-xs-6.inline-block-xs
						img.block(src="img/icons/digital-banking-icon.png")
						h6.text-center DIGITAL BANKING
				a(href='#')
					.col-sm-3.col-xs-6.inline-block-xs
						img.block(src="img/icons/mortgage-icon.png")
						h6.text-center MORTGAGE
				a(href='#')
					.col-sm-3.col-xs-6.inline-block-xs
						img.block(src="img/icons/budget-tool-icon.png")
						h6.text-center BUDGETING TOOL


	section#full-width-ads.imagebg.parallax.height-60.overlay-green(data-overlay="5")
		.background-image-holder
			img(src="/img/phs.jpg")
		.container.pos-vertical-center
			.row.text-center
				.col-sm-12
